#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include <string>

using namespace std;

void mayuscula(string s);

void main()
{
	string palabra;
	getline(cin,palabra);
	cout << palabra<<endl;
	mayuscula(palabra);
	_getwch();
}

void mayuscula(string s)
{
	for (int i = 0; i < s.length(); i++) 
	{
		s[i] = toupper(s[i]);
		cout << s[i];
	}


}
